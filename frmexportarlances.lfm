  object zqExpLances: TZQuery
    Connection = dmGeneral.zcDB
    BeforeOpen = zqExpLancesBeforeOpen
    AfterOpen = zqExpLancesAfterOpen
    UpdateObject = zuExpLances
    CachedUpdates = True
    SQL.Strings = (
      'SELECT 0 as seleccionado, il.* '
      'FROM v_informe_lances il'
      'WHERE idmarea=:idmarea'
      'AND (:filtro=1 OR enviado=0)'
      'ORDER BY fecha'
