    BeforeOpen = zqEnviosEmailBeforeOpen
    SQL.Strings = (
      'SELECT *'
      'FROM v_envios_email'
      'WHERE idmarea=:idmarea'
      ' AND ('
      ':__FILTRO__ IS NULL OR'
      ':__FILTRO__ = '''''
      'OR date_format(fecha, ''%d/%m/%Y'') LIKE CONCAT(''%'',:__FILTRO__,''%'')'
      'OR notas LIKE CONCAT(''%'',:__FILTRO__,''%''))'
      'ORDER BY fecha'
